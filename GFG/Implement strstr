class GfG
{
    //Function to locate the occurrence of the string x in the string s.
    int strstr(String s, String x)
    {
        int len = x.length();
       
       for(int i=0; i<=s.length()-len; i++){
           
           if(s.substring(i,(len+i)).equals(x)){
               return i;
           }
       }
       
       return -1;
    }
}
