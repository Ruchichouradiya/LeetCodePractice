class Solution {
    public int maximumNumberOfStringPairs(String[] words) {
        int max=0;
        for(int i=0;i<words.length;i++){
            for(int j=i+1;j<words.length;j++){
                StringBuilder sb = new StringBuilder(words[j]);
                if(words[i].equals(sb.reverse().toString()))
                    max++;
            }
        }
        return max;
    }
}
